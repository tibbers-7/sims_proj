        <TextBox Name="tbDatum" HorizontalAlignment="Left" Margin="172,211,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbAdresa" HorizontalAlignment="Left" Margin="172,279,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbMail" HorizontalAlignment="Left" Margin="172,341,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <CheckBox Name="checkBoxGuest" Content="" HorizontalAlignment="Left" Margin="172,313,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.362,3.597"/>
