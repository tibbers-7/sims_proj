        <Button Content="Dodaj" HorizontalAlignment="Left" Margin="42,379,0,0" VerticalAlignment="Top" Click="Button_Click" Height="26" Width="60"/>
        <Button Content="Otkazi" HorizontalAlignment="Left" Margin="231,379,0,0" VerticalAlignment="Top" Height="26" Width="61" Click="Button_Click_1"/>
