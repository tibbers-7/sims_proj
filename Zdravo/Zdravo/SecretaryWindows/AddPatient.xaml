        <TextBox Name="tbPrezime" HorizontalAlignment="Left" Margin="172,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox  Name="tbId" HorizontalAlignment="Left" Margin="172,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbUsername" HorizontalAlignment="Left" Margin="172,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
