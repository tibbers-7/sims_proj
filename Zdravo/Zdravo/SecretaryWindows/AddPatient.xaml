        <Label Content="Telefon:&#xD;&#xA;" HorizontalAlignment="Left" Margin="42,178,0,0" VerticalAlignment="Top"/>
        <Label Content="Datum rodjenja:&#xD;&#xA;" HorizontalAlignment="Left" Margin="42,211,0,0" VerticalAlignment="Top"/>
        <Label Content="Pol:&#xD;&#xA;" HorizontalAlignment="Left" Margin="42,245,0,0" VerticalAlignment="Top"/>
        <Label Content="Adresa:&#xD;&#xA;" HorizontalAlignment="Left" Margin="43,279,0,0" VerticalAlignment="Top"/>
