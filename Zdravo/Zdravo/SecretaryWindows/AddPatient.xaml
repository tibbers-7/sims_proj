        <Label Content="JMBG:" HorizontalAlignment="Left" Margin="47,36,0,0" VerticalAlignment="Top" RenderTransformOrigin="4.892,1.893"/>
        <Label Content="Username:&#xD;&#xA;" HorizontalAlignment="Left" Margin="43,94,0,0" VerticalAlignment="Top"/>
