        <TextBox x:Name="tbPrezime" HorizontalAlignment="Left" Margin="123,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" Grid.ColumnSpan="2"/>
        <TextBox IsReadOnly="True" x:Name="tbId" HorizontalAlignment="Left" Margin="123,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" Grid.ColumnSpan="2"/>
        <TextBox x:Name="tbUsername" HorizontalAlignment="Left" Margin="123,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" Grid.ColumnSpan="2"/>
        <TextBox x:Name="tbSifra" HorizontalAlignment="Left" Margin="123,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" Grid.ColumnSpan="2"/>
        <TextBox x:Name="tbTelefon" HorizontalAlignment="Left" Margin="123,159,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" Grid.ColumnSpan="2"/>
