        <Label Content="Username:&#xA;" HorizontalAlignment="Left" Margin="42,92,0,0" VerticalAlignment="Top" Height="42" Width="66"/>
        <Label Content="Sifra:&#xA;" HorizontalAlignment="Left" Margin="42,126,0,0" VerticalAlignment="Top" Height="42" Width="36"/>
