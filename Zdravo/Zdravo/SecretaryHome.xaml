            <Button.Background>
                <ImageBrush ImageSource="/images/cart.png"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="332,583,0,0" VerticalAlignment="Top" Height="30" Width="30">
            <Button.Background>
                <ImageBrush ImageSource="/images/logout.png"/>
