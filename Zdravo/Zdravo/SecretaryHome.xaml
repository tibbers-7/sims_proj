            <Button.Background>
                <ImageBrush ImageSource="/images/meetings.png"/>
            </Button.Background>
        </Button>
        <Button Name="patientsButton" Content="" HorizontalAlignment="Left" Margin="262,64,0,0" VerticalAlignment="Top" Height="100" Width="100" Click="PatientsWindowClick">
